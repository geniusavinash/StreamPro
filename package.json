{
  "name": "streampro",
  "version": "1.0.0",
  "description": "Professional Camera Streaming Platform - Enterprise-grade multi-camera RTMP streaming solution",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm run backend:dev\" \"npm run frontend:dev\"",
    "backend:dev": "cd backend && npm run start:dev",
    "frontend:dev": "cd frontend && npm start",
    "backend:build": "cd backend && npm run build",
    "frontend:build": "cd frontend && npm run build",
    "backend:start": "cd backend && npm run start:prod",
    "frontend:start": "cd frontend && serve -s build",
    "install:all": "npm install && cd backend && npm install && cd ../frontend && npm install --legacy-peer-deps",
    "build:all": "npm run backend:build && npm run frontend:build",
    "test:backend": "cd backend && npm test",
    "test:frontend": "cd frontend && npm test",
    "test:all": "npm run test:backend && npm run test:frontend",
    "lint:backend": "cd backend && npm run lint",
    "lint:frontend": "cd frontend && npm run lint",
    "lint:all": "npm run lint:backend && npm run lint:frontend"
  },
  "keywords": [
    "camera",
    "streaming",
    "rtmp",
    "surveillance",
    "video",
    "monitoring",
    "security",
    "enterprise",
    "professional",
    "react",
    "nestjs",
    "typescript"
  ],
  "author": {
    "name": "Avinash",
    "email": "geniusavinash@example.com",
    "url": "https://github.com/geniusavinash"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/geniusavinash/StreamPro.git"
  },
  "bugs": {
    "url": "https://github.com/geniusavinash/StreamPro/issues"
  },
  "homepage": "https://github.com/geniusavinash/StreamPro#readme",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}